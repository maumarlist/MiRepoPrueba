<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.video.pelisalacarta"
    name="pelisalacarta"
    version="3.9.99"
    provider-name="tvalacarta">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="es">Películas y series a la carta</summary>
    <description lang="es">Películas, series, documentales, anime, ... todo lo que puedas encontrar en Internet ahora desde tu media center. Incluye canales en Español, Italiano, Inglés y Rumano.</description>
    <summary lang="en">Movies and TV Shows on demand</summary>
    <description lang="en">Movies, Series, Documentaries, Anime, ... everything you can find on Internet now on your media center. Includes channels in Spanish, Italian, English and Romanian.</description>
    <platform>all</platform>
  </extension>
  <extension point="xbmc.service" library="library_service.py" start="login|startup">
  </extension>
</addon>

<addon id="plugin.video.popcornflix"
       name="Popcornflix"
       version="6.0.2"
       provider-name="t1m">
  <requires>
    <import addon="xbmc.python" version="2.20.0"/>
    <import addon="script.module.t1mlib" version="1.0.14"/>
  </requires>
  <extension point="xbmc.python.pluginsource"
            library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Popcornflix.com Video Addon</summary>
    <description lang="en">Popcornflix offers a broad collection of great movies you can watch right now</description>
    <disclaimer lang="en">Feel free to use this script. For information visit the wiki.</disclaimer>
    <platform>all</platform>
    <language>en</language>
    <license>GNU GENERAL PUBLIC LICENSE. Version 2, June 1991</license>
    <forum>http://forum.xbmc.org/showthread.php?tid=198931</forum>
    <website>http://www.popcornflix.com</website>
    <email></email>
    <source>https://github.com/learningit/plugin.video.popcornflix</source>
  </extension>
</addon>

<addon id="plugin.video.tvultimate"
       name="Tvultimate"
       version="4.0.0"
       provider-name="Team-Ultimate">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4" />
    <import addon="script.module.requests" />
    <import addon="script.module.httplib2" />
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube" optional="true" />
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson" />
     <import addon="script.module.livestreamer" optional="true"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Tvultimate la Tv GRATIS!  Version 4.0</summary>
    <description>
Disfruta de los mejores canales...
 completamente gratis!...
 Este addon y las listas incluidas son actualizadas
 constantemente por lo que siempre tendremos el mejor
 contenido. Disfrutalo!
    </description>
    <platform>all</platform>
  </extension>
</addon>


<addon id="repository.MiRepoPrueba" name="Repo Josh" version="1.1.0" provider-name="RepoJosh">
	<extension point="xbmc.addon.repository" name="Repo Josh">
		<info compressed="false">https://raw.githubusercontent.com/maumarlist/MiRepoPrueba/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/maumarlist/MiRepoPrueba/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/maumarlist/MiRepoPrueba/master</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>MauMarList official Repo</summary>
   		<description lang="en">Instala LiveTv.</description>
		<disclaimer></disclaimer>
		<platform>all</platform>
	</extension>
</addon>
</addons>
